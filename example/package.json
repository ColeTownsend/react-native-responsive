{
  "name": "ReactNativeResponsive",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "cli": "node node_modules/react-native/local-cli/cli.js",
    "dev:watch": "babel-node ./bin/watch.js",
    "dev:packager": "node node_modules/react-native/local-cli/cli.js start --root ./node_modules/react-native-responsive",
    "dev": "concurrently \"npm run dev:watch\" \"npm run dev:packager\""
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-native": "^0.34.1",
    "react-native-responsive": "../"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "concurrently": "^3.0.0",
    "fs-extra": "^0.30.0",
    "minimatch": "^3.0.3",
    "node-watch": "^0.4.0"
  }
}
